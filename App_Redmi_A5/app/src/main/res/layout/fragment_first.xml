<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/main"
        android:layout_margin="16dp"
        tools:context=".MainActivity"
        tools:ignore="UselessParent">


        <Button
            android:id="@+id/installtermos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/install_termos"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installfm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installfm"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installaurora"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installaurora"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installnewpipe"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installnewpipe"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installjob"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installjob"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installtc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installtc"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installozonbank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installozonbank"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installsberbank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installsberbank"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installtelegram"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installtelegram"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installgnucash"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installgnucash"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installkeychain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/installkeychain"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installdpi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/intalldpi"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installsports"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/intallsports"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installgesture"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/intallgesture"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize" />

        <Button
            android:id="@+id/installhaker"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="48dp"
            android:text="@string/intallhaker"
            tools:ignore="UsingOnClickInXml,VisualLintButtonSize,TouchTargetSizeCheck" />

    </LinearLayout>
</FrameLayout>